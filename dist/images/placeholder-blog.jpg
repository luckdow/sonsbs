<!-- Blog placeholder görseli - JPG compatible SVG -->
<svg width="800" height="400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="400" fill="#3B82F6"/>
  
  <!-- Overlay pattern -->
  <defs>
    <pattern id="dots" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
      <circle cx="10" cy="10" r="1" fill="white" opacity="0.1"/>
    </pattern>
  </defs>
  <rect width="800" height="400" fill="url(#dots)"/>
  
  <!-- Content -->
  <g transform="translate(400, 200)">
    <!-- Blog icon background -->
    <circle cx="0" cy="-20" r="50" fill="white" opacity="0.1"/>
    
    <!-- Blog document icon -->
    <rect x="-25" y="-40" width="50" height="40" rx="6" fill="white" stroke="none"/>
    <rect x="-20" y="-32" width="40" height="2" fill="#3B82F6"/>
    <rect x="-20" y="-24" width="30" height="2" fill="#8B5CF6"/>
    <rect x="-20" y="-16" width="35" height="2" fill="#6B7280"/>
    <rect x="-20" y="-8" width="25" height="2" fill="#6B7280"/>
    
    <!-- Title -->
    <text x="0" y="50" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="28" font-weight="bold">
      GATE Transfer Blog
    </text>
    
    <!-- Subtitle -->
    <text x="0" y="80" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="16" opacity="0.9">
      Transfer Rehberi &amp; Seyahat İpuçları
    </text>
    
    <!-- Decorative elements -->
    <circle cx="-100" cy="-100" r="20" fill="white" opacity="0.05"/>
    <circle cx="120" cy="-80" r="15" fill="white" opacity="0.05"/>
    <circle cx="-80" cy="120" r="25" fill="white" opacity="0.05"/>
    <circle cx="90" cy="110" r="18" fill="white" opacity="0.05"/>
  </g>
</svg>
